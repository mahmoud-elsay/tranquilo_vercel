{
    "version": 2,
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python"
        }
    ],
    "functions": {
        "api/**/*.py": {
            "runtime": "python3.9"
        }
    },
    "ignore": [
        "*.csv",
        "*.log",
        "*.md",
        "__pycache__/",
        "*.pkl",
        "*.zip"
    ]
}